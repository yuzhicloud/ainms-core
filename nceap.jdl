entity CountryStistics {
  countryName String required,
  totalCount Long,
  onlineCount Long,
  offlineCount Long,
  otherCount Long,
  statisticDate LocalDate,
  statisticTime Instant
}

entity ProvinceStistics {
  provinceName String required,
  totalCount Long,
  onlineCount Long,
  offlineCount Long,
  otherCount Long,
  statisticDate LocalDate,
  statisticTime Instant
}

entity PowerPlantStistics {
  powerplantName String required,
  totalCount Long,
  onlineCount Long,
  offlineCount Long,
  otherCount Long,
  statisticDate LocalDate,
  statisticTime Instant
}

relationship OneToMany {
    CountryStistics{province} to ProvinceStistics{country},
    ProvinceStistics{station} to PowerPlantStistics{province}
}
